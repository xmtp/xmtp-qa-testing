      "id": 8186190636625556,
      "definition": {
        "title": "Server Call Average (ms)",
        "type": "query_value",
        "requests": [
          {
            "response_format": "scalar",
            "queries": [
              {
                "name": "query1",
                "data_source": "metrics",
                "query": "avg:xmtp.sdk.duration{metric_type:network,network_phase:server_call,$env,$geo,$test,$libxmtp,$members}",
                "aggregator": "avg"
              }
            ],
            "formulas": [{ "formula": "query1" }],
            "conditional_formats": [
              { "comparator": "<=", "value": 100, "palette": "white_on_green" },
              {
                "comparator": "<=",
                "value": 300,
                "palette": "white_on_yellow"
              },
              { "comparator": ">", "value": 300, "palette": "white_on_red" }
            ]
          }
        ],
        "autoscale": true,
        "precision": 2
      },
      "layout": { "x": 0, "y": 0, "width": 2, "height": 2 }
    },
